<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.knime.core.PortType">
      <portType
            color="#ff6f00"
            hidden="false"
            name="TensorFlow 2 Model"
            objectClass="org.knime.dl.tensorflow2.base.portobjects.TF2NetworkPortObject"
            objectSerializer="org.knime.dl.tensorflow2.base.portobjects.TF2NetworkPortObject$Serializer"
            specClass="org.knime.dl.tensorflow2.base.portobjects.TF2NetworkPortObjectSpec"
            specSerializer="org.knime.dl.tensorflow2.base.portobjects.TF2NetworkPortObjectSpec$Serializer">
      </portType>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            icon="icons/tensorflow2.png"
            level-id="tensorflow2"
            locked="true"
            name="TensorFlow 2"
            path="/labs/deeplearning">
      </category>
   </extension>
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/labs/deeplearning/tensorflow2"
            deprecated="false"
            factory-class="org.knime.dl.tensorflow2.base.nodes.io.filehandling.tfnetwork.reader.TF2ReaderNodeFactory">
      </node>
      <node
            category-path="/labs/deeplearning/tensorflow2"
            deprecated="false"
            factory-class="org.knime.dl.tensorflow2.base.nodes.io.filehandling.tfnetwork.writer.TF2WriterNodeFactory">
      </node>
      <node
            category-path="/labs/deeplearning/tensorflow2"
            deprecated="false"
            factory-class="org.knime.dl.tensorflow2.base.nodes.executor.TF2ExecutorNodeFactory">
      </node>
   </extension>
   <extension
         point="org.knime.dl.DLExecutionContext">
      <DLExecutionContext
            DLExecutionContext="org.knime.dl.tensorflow2.core.execution.TF2ExecutionContext">
      </DLExecutionContext>
   </extension>
   <extension
         point="org.knime.dl.python.DLPythonNetworkLoader">
      <DLPythonNetworkLoader
            DLPythonNetworkLoader="org.knime.dl.tensorflow2.core.TF2NetworkLoader">
      </DLPythonNetworkLoader>
   </extension>
   <extension
         point="org.knime.python.modules">
      <directory
            path="py/">
      </directory>
   </extension>
   <extension
         point="org.knime.dl.python.DLPythonModuleDependency">
      <DLPythonModuleDependency
            DLPythonModuleDependency="org.knime.dl.tensorflow2.core.TF2PythonModuleDependency">
      </DLPythonModuleDependency>
   </extension>
</plugin>
